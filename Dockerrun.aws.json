{
  "AWSEBDockerrunVersion": 1,
  "volumes": [
    {
      "name": "api-prod",
      "host": {
        "sourcePath": "/api"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "api-prod",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 4000
        }
      ]
    }
  ]
}
